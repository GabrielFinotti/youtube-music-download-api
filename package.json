{
  "name": "ytune-api",
  "version": "1.0.0",
  "description": "YTune API - Uma API moderna para download de m√∫sicas do YouTube",
  "main": "dist/server.cjs",
  "type": "commonjs",
  "scripts": {
    "build": "tsup src/server.ts --format cjs --dts --clean",
    "start": "node dist/server.cjs",
    "dev": "tsx watch src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "youtube",
    "music",
    "download",
    "api",
    "mp3",
    "ytdl",
    "ffmpeg"
  ],
  "author": "Gabriel H. Finotti",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "youtube-dl-exec": "^3.0.25"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
